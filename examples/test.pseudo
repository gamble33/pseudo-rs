PROCEDURE Main
    OUTPUT 997 DIV 2
    OUTPUT IsPrime(997)
ENDPROCEDURE

FUNCTION IsPrime(N : INTEGER) RETURNS BOOLEAN
    DECLARE I : INTEGER
    DECLARE Limit : INTEGER
    Limit <- N DIV 2
    I <- 2
    WHILE I < Limit
        IF N MOD I = 0 THEN
            RETURN FALSE
        ENDIF
        I <- I + 1
    ENDWHILE
    RETURN TRUE
ENDFUNCTION
